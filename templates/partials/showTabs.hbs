<div class="tabContainer">
    <ul class="tabs" data-tab role="tablist">
        <li class="tab-title active" role="presentation"><a href="#panel2-1" role="tab" tabindex="0" aria-selected="true" aria-controls="panel2-1"><h3>Show notes</h3></a></li>
        <li class="tab-title" role="presentation"><a href="#panel2-2" role="tab" tabindex="0" aria-selected="false" aria-controls="panel2-2"><h3>Transcript</h3></a></li>
        <li class="tab-title" role="presentation"><a href="#panel2-3" role="tab" tabindex="0" aria-selected="false" aria-controls="panel2-3"><h3>Comments</h3></a></li>
    </ul>
</div>
<div class="tabs-content">
    <section role="tabpanel" aria-hidden="false" class="content active" id="panel2-1">
        {{{showNotes}}}
    </section>
    <section role="tabpanel" aria-hidden="true" class="content" id="panel2-2">
        {{{transcript}}}
    </section>
    <section role="tabpanel" aria-hidden="true" class="content" id="panel2-3">
        {{> comments}}
    </section>
</div>